<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="choose-a"
           fetch="https://github.com"
           review="https://review.choose-a.name" />

  <!-- remove unused/forked AOSP projects -->
  <include name="remove.xml" />

  <!-- choose-projects -->
  <project name="choose-a/android" path="android" remote="choose-a" revision="choose-9.0" />
  <project name="choose-a/android_bionic" path="bionic" remote="choose-a" revision="choose-9.0" />

  <project path="build/make" name="choose-a/android_build" remote="choose-a" revision="choose-9.0" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="choose-a/android_build_soong" remote="choose-a" revision="choose-9.0" groups="pdk">
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
